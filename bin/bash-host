#!/bin/bash

######################################################
# Machine specific stuff for a remote host

case "$TERM" in
xterm*|rxvt*)
	PS1='${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w\[\033[00m\]\$ '
    ;;
*)
    ;;
esac

alias ls='ls'
alias grep='grep'

