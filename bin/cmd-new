#!/usr/bin/python3
# Create a new python command script

from sys import argv
from command import cmd_new


# When run stand alone then do the test
if __name__ == "__main__":
    if len(argv)>1:
        if len(argv)>2:
            if len(argv)>3:
                cmd_new(argv[1], argv[2], argv[3])
            else:
                cmd_new(argv[1], argv[2])
        else:
            cmd_new(argv[1])
    else:
        print ('Need a file')


  
