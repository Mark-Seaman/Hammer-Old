#!/bin/bash
# Copy the repositories to another computer

ip=$1
echo "Copy the repositories to $ip computer"

cd /home/seaman
{
    cmd='mkdir -p LiveSync LiveSync/Music LiveSync/Music'
    echo "$cmd"
    echo "$cmd" | ssh $1 > /dev/null

    date
    cptree ~/LiveSync/Music $ip:~/LiveSync/Music  &&
    date
    echo "clone-music Successful"
}| mklog clone-music