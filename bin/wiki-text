#!/usr/bin/python
# Wrap lines of text

from sys import stdin

# Split words on a line
def words(line):
    return line.split(' ')

# Join lines into a string of text
def text(lines):
    return '\n'.join(lines)

# Divide up the lines
def split_lines(text):
    return text.split('\n')
   
# Print a text file
def paragraphs(text):
    nl='**new-line**'
    text = text.replace('\n\n\n\n', nl)
    text = text.replace('\n\n\n', nl)
    text = text.replace('\n\n', nl)
    text = text.replace('\n', ' ')
    text = text.replace(nl, '\n')
    return split_lines(text)

# Wrap a single line at 100 characters
def wrap_line(lines):
    result = []
    line = ''
    for w in words(lines):
        if len(line+w) > 100:
            result.append(line)
            line = ''
        line += w + ' ' 
    result.append(line)
    return text(result)+'\n'

# Remove the muse tag from the first line
def remove_muse(line):
    line = line.replace ('-*-muse-*-', '')
    line = line.replace ('-*- muse -*-', '') 
    line = line.replace ('* ', '')
    line = line.rstrip()
    return line

# Convert the text by wrapping lines individually
def wrap_lines(lines):
    lines = paragraphs(lines)
    lines = map(remove_muse,lines)
    lines = map(wrap_line, lines)
    return  text(lines)

print wrap_lines(stdin.read())

