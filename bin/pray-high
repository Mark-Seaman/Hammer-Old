#!/bin/bash
# List the highs and lows

echo "Spiritual Highlight:"
cat  $br/Highlights | 
grep -a High:       |
sed 's/-.*//'       |
sed 's/\..*//'      |
sed 's/High: //'    | 
random

# Used to format the old file
# cat $br/HighsAndLows | perl -pe 's/ (\w*:)/\n\n\1/g' | wiki-text > $br/Highlights
