#!/bin/bash
# Total the numbers in column 3



cat <<EOF > xxx
08/29/13 WME 5587.50
08/12/13 CNE 4350.00
08/05/13 WME 3992.50

07/29/13 WME 4725.00    
07/15/13 WME 2737.50    
07/03/13 CNE 5137.50

06/21/13 WME 4060.00
06/14/13 WME 3607.50
06/12/13 SA  427.50  

05/23/13 WME 3077.50
05/13/13 CNE 4931.25    
05/06/13 WME 3906.25
EOF

cat <<EOF > xxx1
04/23/13 CNE 8625.00
04/22/13 WME 3468.75
04/16/13 WME 3387.50

03/25/13 WME 3617.50
03/15/13 WME 3766.25

02/25/13 CNE 4462.50
02/25/13 WME 4655.00    
02/11/13 CNE 4687.50
02/07/13 WME 5395.00    

01/28/13 WME 6826.25
01/07/13 WME 3667.50
EOF

cat <<EOF > yyy
08/01/13 240.00
06/19/13 105.00  
06/05/13 135.00
05/17/13 195.00
05/02/13 375.00  
EOF

cat <<EOF > yyy1
04/18/13 997.50
04/02/13 699.00  
04/02/13 525.00  
03/20/13 330.00
02/19/13 1065.00
02/07/13 622.50
01/17/13 885.00  
01/08/13 427.50
EOF

echo `cat xxx| awk '{sum+=$3} END {print sum}'` "Total"
echo `cat yyy| awk '{sum+=$2} END {print sum}'` "Total"
