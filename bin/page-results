#!/usr/bin/env python
# Create a page to display search results

from sys import stdin,argv
from re import match, compile,IGNORECASE

if len(argv)>2:
    print 'usage: cat page | page-results brain'
    exit (1)

area = argv[1]
print '* Search Results -- %s *                        -*- muse -*-\n'%area

for line in  stdin.read().split('\n'):
    c = compile(r'(.*):(.*)')
    m = c.match(line)
    if m:
        print "[[%s][%s]]"%(area+'/'+m.group(1),m.group(1)), m.group(2)

