#!/usr/bin/python
# List all my contacts

from files import read_file

contacts = read_file ('/home/seaman/Documents/Brain/EmailContacts')
contacts = map (lambda s: s.split(','), contacts)
contacts = filter(lambda c: len(c[0])>1 and len(c[1])>1, contacts)
for c in contacts:
    print "%-40s"%c[1][1:-1], c[0][1:-1]