#!/bin/bash
# Create a new Django application.

project=$1
appName=$2
className=$3

cd /home/seaman/Code/
dj-app      $project $appName $className
dj-script   $project $appName $className
dj-settings $project $appName $className
dj-urls     $project $appName $className
dj-start    $project $appName $className

