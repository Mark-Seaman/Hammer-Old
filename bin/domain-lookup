#!/usr/bin/env python
# Lookup each domain listed in the input

from os import system
from sys import stdin

hosts = '''
    act2learn.org	
    agilebusiness.us
    comsystem.us	
    exteriorbrain.com	
    fightbitrot.com	
    markseaman.org	
    markseaman.info	
    mybookonline.org	
    seamansguide.com	
    seamanslog.com	
    seamantech.com	
    seaman-tech.com	
    shrinking-world.org	
    spiritual-things.org	
    the-core-idea.com	
    the-very-idea.us	
    world-class-software.com	
'''

for h in hosts.split("\n")[1:-1]:
    system('nslookup ' + h)


