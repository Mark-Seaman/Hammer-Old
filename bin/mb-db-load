#!/bin/bash
# Create a new mybook Django application

project=mybook
projectdir=/home/seaman/Documents/Code/mybook
dbfile=$projectdir/data/seaman_mybook.sql

cd $projectdir
echo "Reload database for $project"

# Create the database
sqlrmdb  seaman_$project 
sqlnew   $project $dbfile

# Sync the database
#manage.py syncdb


