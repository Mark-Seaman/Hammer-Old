#!/usr/bin/python
# Show the mileage for this year

m = open('Mileage-2013').read().split('\n')
m = map (lambda x: x.split(','), m)
m = filter(lambda x:len(x)>2, m)
m = map(lambda x: int(x[2].strip()), m)
print 'Drove', len(m), 'days'
print reduce(lambda x,y: x+y, m), "miles"
print 'Average trip', reduce(lambda x,y: x+y, m)/len(m)


