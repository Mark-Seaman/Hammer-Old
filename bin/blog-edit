#!/bin/bash
# Show the articles for Seamans Log
echo  Show the articles for Seamans Log

# Select the article
article=$1
[[ -f "$y/SeamansLog/$article"   ]] && e $y/SeamansLog/$article   && exit
[[ -f "$y/SpiritualLog/$article" ]] && e $y/SpiritualLog/$article && exit

# Select the blog
blog=SeamansLog
[[ "$1" != "" ]] && [[ -d "$y/$1/" ]] && blog=$1
e $y/$blog/


